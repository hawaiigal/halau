<%= form_with model: haumana do |form| %>
  <div>
      <%= form.label :name %><br>
      <%= form.text_field :name %>
      <%= haumana.errors.full_messages_for(:name).each do |message| %>
          <div><%= message %></div>
      <% end %>
  </div>

  <div>
      <%= form.submit %>
  </div>

<% end %>